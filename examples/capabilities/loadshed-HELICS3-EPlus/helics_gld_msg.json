{
  "name": "gridlabdSimulator1",
  "core_name": "GLD-core",
  "period": "15s",
  "loglevel": "warning",
  "endpoints": [
    {
      "name": "sw_status",
      "global": false,
      "type": "string",
      "info": {
        "object": "sw_loadshed",
        "property": "phase_A_state"
      }
    }
  ],
  "subscriptions": [
    {
      "key": "eplus_agent/power_A",
      "type": "complex",
      "info": {
        "object": "Eplus_load",
        "property": "constant_power_A"
      }
    },
    {
      "key": "eplus_agent/power_B",
      "type": "complex",
      "info": {
        "object": "Eplus_load",
        "property": "constant_power_B"
      }
    },
    {
      "key": "eplus_agent/power_C",
      "type": "complex",
      "info": {
        "object": "Eplus_load",
        "property": "constant_power_C"
      }
    },
    {
      "key": "eplus_agent/bill_mode",
      "type": "string",
      "info": {
        "object": "Eplus_meter",
        "property": "bill_mode"
      }
    },
    {
      "key": "eplus_agent/price",
      "type": "double",
      "info": {
        "object": "Eplus_meter",
        "property": "price"
      }
    },
    {
      "key": "eplus_agent/monthly_fee",
      "type": "double",
      "info": {
        "object": "Eplus_meter",
        "property": "monthly_fee"
      }
    }
  ]
}
